    <string name="settings_title">News Settings</string>


    <!-- String array for section by label in setting menu -->
    <string-array name="settings_order_by_labels">
        <item>@string/settings_order_by_newest_news_label</item>
        <item>@string/settings_order_by_oldest_news_label</item>
